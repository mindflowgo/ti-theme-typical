{
  "doctype": "html5",
  "favicon": "typical/assets/images/favicon.ico",
  "meta": [
    {
      "name": "Content-type",
      "content": "text/html; charset=utf-8",
      "type": "equiv"
    },
    {
      "name": "X-UA-Compatible",
      "content": "IE=edge",
      "type": "equiv"
    },
    {
      "name": "viewport",
      "content": "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no",
      "type": "name"
    }
  ],
  "style": [
    {
      "path": "typical/assets/css/app.css",
      "name": "app-css"
    }
  ],
  "script": [
    {
      "path": "typical/assets/js/app.js",
      "name": "app-js"
    }
  ],
  "bundles": [
    {
      "type": "scss",
      "files": "~/themes/typical/assets/src/scss/app.scss",
      "destination": "~/themes/typical/assets/css/app.css"
    }
  ]
}